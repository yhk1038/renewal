<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie9"><![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />

    <title>Renewal</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

    <!-- Vendor CSS -->
    <link href="/template/vendors/bower_components/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
    <link href="/template/vendors/bower_components/animate.css/animate.min.css" rel="stylesheet">
    <link href="/template/vendors/bower_components/bootstrap-sweetalert/lib/sweet-alert.css" rel="stylesheet">
    <link href="/template/vendors/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css" rel="stylesheet">
    <link href="/template/vendors/bower_components/lightgallery/light-gallery/css/lightGallery.css" rel="stylesheet">
    <link href="/template/vendors/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet">
    
    <!-- CSS -->
    <link href="/template/css/app.min.1.css" rel="stylesheet">
    <link href="/template/css/app.min.2.css" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="http://kenwheeler.github.io/slick/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="http://kenwheeler.github.io/slick/slick/slick-theme.css" />

    <%= render partial: '/layouts/partials/layout_config_script_variables' %>
    <script src="/template/vendors/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/template/vendors/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <script type="text/javascript" src="http://kenwheeler.github.io/slick/slick/slick.min.js"></script>
</head>

<body class="<%= 'no-delivery' unless @delivery_display %> <%= 'no-carousel' unless @layout_carousel_display %>">

<%#= render partial: '/layouts/partials/tagline' %>
<%= render partial: '/layouts/partials/delivery' if @delivery_display %>
<%= render partial: '/layouts/partials/navbar' %>

<section id="main" data-layout="layout-1" data-template="default" class="">

    <%= render partial: '/layouts/partials/flash' %>

    <% if @layout_carousel_display %>
        <div id="carousel-wrapper">
            <%= render partial: '/layouts/partials/carousel' %>
        </div>
    <% else %>
        <div id="replace-carousel-wrapper"></div>
    <% end %>

    <%#= render partial: '/layouts/partials/sidebar' %>

    <section id="content">
        <%= yield %>
    </section>
</section>

<%= render partial: '/layouts/partials/footer' %>

<!-- Page Loader -->
<%= render partial: '/layouts/partials/page_loader' %>

<!-- Modals -->
<%= render partial: '/layouts/partials/modals/modal_bundle' %>

<!-- Older IE warning message -->
<%= render partial: '/layouts/partials/ie_warning' %>

<%= render partial: '/layouts/partials/javascript_libraries' %>
<%= javascript_include_tag 'application' %>
</body>
</html>
